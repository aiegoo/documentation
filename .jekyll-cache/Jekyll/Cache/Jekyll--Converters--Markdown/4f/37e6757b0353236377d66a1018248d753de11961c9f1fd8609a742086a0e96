    <span class="n">frame1</span> <span class="o">=</span> <span class="n">cv2</span><span class="p">.</span><span class="n">resize</span><span class="p">(</span><span class="n">frame1</span><span class="p">,(</span><span class="mi">400</span><span class="p">,</span><span class="mi">500</span><span class="p">))</span>
