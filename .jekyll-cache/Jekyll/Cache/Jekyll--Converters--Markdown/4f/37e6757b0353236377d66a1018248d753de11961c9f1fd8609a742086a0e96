        <span class="n">lmain</span><span class="p">.</span><span class="n">configure</span><span class="p">(</span><span class="n">image</span><span class="o">=</span><span class="n">imgtk</span><span class="p">)</span>
        <span class="n">lmain</span><span class="p">.</span><span class="n">after</span><span class="p">(</span><span class="mi">10</span><span class="p">,</span> <span class="n">show_vid</span><span class="p">)</span>


<span class="k">def</span> <span class="nf">show_vid2</span><span class="p">():</span>
